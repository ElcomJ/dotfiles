{
  // Icons
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
    },

    // Language Server
    "languageserver": {

    },

    // Format Filetypes
    "coc.preferences.formatOnSaveFiletypes": [
      "xml",
      "yaml",
      "yml",
      "json",
      "html",
      "css",
      "javascript",
      "markdown"
      ],

    // Coc Diagnostics
    "diagnostic.displayByAle": true,
    "diagnostic.refreshOnInsertMode": true,

    // Coc Preferences
    "coc.preferences.colorSupport": true,
    "coc.preferences.formatOnSave": true,

    // Inlay Hint
    "inlayHint.display": true,
    "inlayHint.refreshOnInsertMode": true,

    // Spell Checker
    "cSpell.language": "en",
    "cSpell.enabledLanguageIds": [
      "markdown",
      "text"
    ],

    // Coc Explorer
    "explorer.width": 30,
    "explorer.position": "left",
    "explorer.mouseMode": "none",
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.source" : "nvim-web-devicons",
    "explorer.previewAction.onHover": false,
    "explorer.expandStores": false,
    "explorer.keyMappings.global": {
    "<cr>": ["expandable?",  ["expanded?", "collapse", "expand"], "open"],
    "rr" : "rename",
    "af" : "addFile",
    "ad" : "addDirectory",
    "df" : "delete",
    "v"  : "open:vsplit",
    "h"  : "open:split"
    },

    // Git
    "explorer.git.enable": true,
    "explorer.git.icon.status.added": "✚",
    "explorer.git.icon.status.deleted": "✖",
    "explorer.git.icon.status.renamed": "➜",
    "explorer.git.icon.status.unmerged": "≠",
    "explorer.git.icon.status.untracked": "?",
    "explorer.git.icon.status.modified": "✹",
    "explorer.git.icon.status.ignored": "☒",
    "explorer.git.icon.status.mixed": "*",
    "snippets.ultisnips.pythonPrompt": false

}
